{
  "name": "server",
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    "./alchemy": "./alchemy.run.ts"
  },
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "alchemy dev --app dono-server",
    "destroy": "alchemy destroy --app dono-server",
    "deploy:dev": "cross-env NODE_ENV=dev alchemy deploy --app dono-server --stage dev ",
    "destroy:dev": "cross-env NODE_ENV=dev alchemy destroy --app dono-server --stage dev"
  },
  "dependencies": {
    "@dono/api": "workspace:*",
    "@dono/auth": "workspace:*",
    "@dono/db": "workspace:*",
    "@dono/env": "workspace:*",
    "@dono/stores": "workspace:*",
    "@livestore/sync-cf": "catalog:",
    "@orpc/openapi": "catalog:",
    "@orpc/server": "catalog:",
    "@orpc/zod": "catalog:",
    "better-auth": "catalog:",
    "dotenv": "catalog:",
    "hono": "catalog:",
    "y-durableobjects": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "catalog:",
    "@dono/config": "workspace:*",
    "@types/node": "^22.13.11",
    "alchemy": "catalog:",
    "tsdown": "^0.16.5",
    "typescript": "catalog:",
    "wrangler": "^4.54.0"
  }
}
